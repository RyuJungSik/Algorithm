   "source": [
    "n = int(input())\n",
    "a = list(map(int,input().split()))\n",
    "d = [0]*n\n",
    "for i in range(n-1, -1, -1):\n",
    "    d[i] = 1\n",
    "    for j in range(i+1, n):\n",
    "        if a[i] > a[j] and d[i] < d[j]+1:\n",
    "            d[i] = d[j]+1\n",
    "        print(i, d)\n",
    "print(max(d))\n"
   ]
