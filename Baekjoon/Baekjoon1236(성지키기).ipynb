    "n, m=map(int, input().split())\n",
    "arr=[]\n",
    "col=set()\n",
    "row=set()\n",
    "for _ in range(n):\n",
    "    arr.append(list(input()))\n",
    "\n",
    "for i in range(0,n):\n",
    "    for j in range(0,m):\n",
    "        if arr[i][j]=='X':\n",
    "            col.add(i)\n",
    "            row.add(j)\n",
    "print(max(n-len(col), m-len(row)))"
   ]
