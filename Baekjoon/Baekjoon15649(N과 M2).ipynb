    "import itertools\n",
    "n,m=map(int, input().split())\n",
    "arr=[i for i in range(1,n+1)]\n",
    "com=list(itertools.combinations(arr,m))\n",
    "for i in com:\n",
    "    for j in i:\n",
    "        print(j, end=\" \")\n",
    "    print()"
