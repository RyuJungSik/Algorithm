    "n=int(input())\n",
    "dp=list(map(int, input().split()))\n",
    "dp.insert(0,0)\n",
    "\n",
    "for i in range(1,n+1):\n",
    "    for j in range(i):\n",
    "        dp[i]=min(dp[i], dp[i-j]+dp[j])\n",
    "print(dp[n])"
   ]
