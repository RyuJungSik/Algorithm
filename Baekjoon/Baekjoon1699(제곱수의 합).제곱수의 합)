   "source": [
    "import math\n",
    "n=int(input())\n",
    "maxV=100000\n",
    "dp=[maxV]*(maxV)\n",
    "dp.insert(0,0)\n",
    "dp[1]=1\n",
    "for i in range(2,maxV+1):\n",
    "    sq=int(math.sqrt(i))\n",
    "    for j in range(sq,0,-1):\n",
    "        dp[i]=min(dp[i], dp[i-j*j]+1)\n",
    "\n",
    "\n",
    "print(dp[n])"
   ]
  }
