   "source": [
    "import heapq\n",
    "\n",
    "heap=[]\n",
    "\n",
    "\n",
    "n=int(input())\n",
    "\n",
    "for _ in range(n):\n",
    "    x=int(input())\n",
    "    heapq.heappush(heap, x)\n",
    "\n",
    "answer=0\n",
    "while len(heap)!=1:\n",
    "    one=heapq.heappop(heap)\n",
    "    two=heapq.heappop(heap)\n",
    "    result=one+two\n",
    "    answer+=result\n",
    "    heapq.heappush(heap, result)\n",
    "print(answer)"
   ]
