    "n=int(input())\n",
    "maxV=91\n",
    "dp=[[0 for _ in range(2)] for _ in range(maxV)]\n",
    "dp[1][1]=1\n",
    "dp[2][0]=1\n",
    "dp[2][1]=0\n",
    "\n",
    "for i in range(3,maxV):\n",
    "    dp[i][0]=dp[i-1][0]+dp[i-1][1]\n",
    "    dp[i][1]=dp[i-1][0]\n",
    "    \n",
    "print(dp[n][0]+dp[n][1])"
   ]
