   "source": [
    "t=int(input())\n",
    "\n",
    "for _ in range(t):\n",
    "    n=int(input())\n",
    "    arr=list(map(int,input().split()))\n",
    "    dp=[0]*n\n",
    "    dp[0]=arr[0]\n",
    "    for i in range(1,n):\n",
    "        dp[i]=max(dp[i-1]+arr[i],arr[i])\n",
    "    print(max(dp))"
   ]
  }
