   "source": [
    "from itertools import permutations\n",
    "\n",
    "n=int(input())\n",
    "a=list(map(int,input().split()))\n",
    "answer=0\n",
    "\n",
    "for per in permutations(a,n):\n",
    "    tmp=0\n",
    "    for i in range(n-1):\n",
    "        tmp+=abs(per[i]-per[i+1])\n",
    "    answer=max(tmp,answer)\n",
    "print(answer)"
   ]
  }
