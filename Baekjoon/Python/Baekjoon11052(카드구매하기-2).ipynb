    "n=int(input())\n",
    "arr=list(map(int,input().split()))\n",
    "arr=[0]+arr\n",
    "\n",
    "dp=[0]*10001\n",
    "dp[1]=arr[1]\n",
    "\n",
    "for i in range(2,1001):\n",
    "    for j in range(len(arr)):\n",
    "        if i-j>=0:\n",
    "            dp[i]=max(dp[i],dp[i-j]+arr[j])\n",
    "print(dp[n])"
   ]
  }
