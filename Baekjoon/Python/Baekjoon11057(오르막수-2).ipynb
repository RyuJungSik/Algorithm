   "source": [
    "n=int(input())\n",
    "\n",
    "dp=[[0]*1001 for _ in range(10)]\n",
    "for i in range(10):\n",
    "    dp[i][1]=1\n",
    "for i in range(2,1001):\n",
    "    for j in range(10):\n",
    "        for k in range(j+1):\n",
    "            dp[j][i]+=dp[k][i-1]%10007\n",
    "answer=0\n",
    "for i in range(10):\n",
    "    answer+=dp[i][n]\n",
    "print(answer%10007)"
   ]
  }
