   "source": [
    "n=int(input())\n",
    "dp=[[0 for _ in range(3)] for _ in range(n+1)]\n",
    "dp[1][0]=1\n",
    "dp[1][1]=1\n",
    "dp[1][2]=1\n",
    "\n",
    "for i in range(2,n+1):\n",
    "    dp[i][0]=(dp[i-1][1]+dp[i-1][2])%9901\n",
    "    dp[i][1]=(dp[i-1][0]+dp[i-1][2])%9901\n",
    "    dp[i][2]=(dp[i-1][0]+dp[i-1][1]+dp[i-1][2])%9901\n",
    "print(sum(dp[n])%9901)"
   ]
  }
