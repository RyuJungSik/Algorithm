   "source": [
    "n=int(input())\n",
    "t=[0]\n",
    "p=[0]\n",
    "dp=[0]*(n+2)\n",
    "\n",
    "for _ in range(n):\n",
    "    a,b=map(int,input().split())\n",
    "    t.append(a)\n",
    "    p.append(b)\n",
    "t.append(0)\n",
    "p.append(0)\n",
    "for i in range(1,n+2):\n",
    "    for j in range(i):\n",
    "        if j+t[j]<=i:\n",
    "            dp[i]=max(dp[i],dp[j]+p[j])\n",
    "print(max(dp))"
   ]
  }
