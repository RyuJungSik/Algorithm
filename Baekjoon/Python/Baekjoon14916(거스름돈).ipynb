    "n=int(input())\n",
    "\n",
    "answer=0\n",
    "\n",
    "for i in range(n//5,-1,-1):\n",
    "    if ((n-(5*i))%2)==0:\n",
    "        answer+=i\n",
    "        answer+=(n-(5*i))//2\n",
    "        break\n",
    "    else:\n",
    "        continue\n",
    "        \n",
    "if answer==0:\n",
    "    print(-1)\n",
    "else:\n",
    "    print(answer)"
