    "from itertools import combinations\n",
    "\n",
    "n,m=map(int, input().split())\n",
    "arr=[]\n",
    "for i in range(1,n+1):\n",
    "    arr.append(i)\n",
    "for i in combinations(arr,m):\n",
    "    for j in i:\n",
    "        print(j, end=\" \")\n",
    "    print()"
   ]
