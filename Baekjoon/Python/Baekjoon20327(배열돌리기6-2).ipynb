    "import math\n",
    "\n",
    "n=int(input())\n",
    "dp=[100000]*100001\n",
    "\n",
    "dp[0]=0\n",
    "dp[1]=1\n",
    "\n",
    "for i in range(1,n+1):\n",
    "    if i**2<n+1:\n",
    "        dp[i**2]=1\n",
    "    for j in range(1,i//2+1):\n",
    "        dp[i]=min(dp[i],dp[j]+dp[i-j])\n",
    "print(dp[n])"
   ]
