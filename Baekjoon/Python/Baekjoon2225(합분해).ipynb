   "source": [
    "mod=1000000000\n",
    "n,k=map(int, input().split())\n",
    "d=[[0]*(n+1) for _ in range(k+1)]\n",
    "d[0][0]=1\n",
    "\n",
    "for i in range(1,k+1):\n",
    "    for j in range(0,n+1):\n",
    "        for l in range(0, j+1):\n",
    "            d[i][j]+=d[i-1][j-l]\n",
    "        d[i][j]%=mod\n",
    "print(d[k][n])"
   ]
  }
