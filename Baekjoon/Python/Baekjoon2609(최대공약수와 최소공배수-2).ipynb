   "source": [
    "n,m=map(int,input().split())\n",
    "\n",
    "a,b=max(n,m),min(n,m)\n",
    "\n",
    "\n",
    "for i in range(b+1,0,-1):\n",
    "    if a%i==0 and b%i==0:\n",
    "        ans1=i\n",
    "        break\n",
    "print(ans1)\n",
    "ans2=(a//ans1)*(b//ans1)*ans1\n",
    "print(ans2)"
   ]
  }
