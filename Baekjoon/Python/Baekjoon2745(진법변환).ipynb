   "source": [
    "n,b=input().split()\n",
    "StoN=dict()\n",
    "ans=0\n",
    "for i in range(0,10):\n",
    "    StoN[str(i)]=i\n",
    "for i in range(ord('A'),ord('Z')+1):\n",
    "    StoN[chr(i)]=i-55\n",
    "\n",
    "    \n",
    "for i in range(len(n)):\n",
    "    ans+=int(b)**(i)* StoN[n[len(n)-i-1]]\n",
    "\n",
    "print(ans)"
   ]
  }
