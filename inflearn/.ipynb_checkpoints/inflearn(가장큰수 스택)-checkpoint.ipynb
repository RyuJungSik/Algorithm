    "n, m=input().split()\n",
    "n=list(n)\n",
    "m=int(m)\n",
    "stack=[]\n",
    "\n",
    "for i in n:\n",
    "    while stack and m>0  and stack[-1]<i:\n",
    "        stack.pop()\n",
    "        m-=1\n",
    "    stack.append(i)\n",
    "print(m)\n",
    "if m>0:\n",
    "    stack=stack[:-m]\n",
    "print(''.join(stack))        \n"
   ]
  }
