    "from itertools import combinations\n",
    "\n",
    "def solution(numbers):\n",
    "    answer = []\n",
    "    combi=[]\n",
    "    combi=list(combinations(numbers, 2))\n",
    "    for i in combi:\n",
    "        if sum(i) not in answer:\n",
    "            answer.append(sum(i))\n",
    "    answer.sort()\n",
    "    return answer"
   ]
  }
