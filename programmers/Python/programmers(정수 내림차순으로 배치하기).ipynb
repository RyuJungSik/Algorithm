    "def solution(n):\n",
    "    answer = 0\n",
    "    a=(int(math.sqrt(n)))\n",
    "    \n",
    "    if a*a==n:\n",
    "        answer=(a+1)*(a+1)\n",
    "    else:\n",
    "        answer=-1\n",
    "    return answer"
