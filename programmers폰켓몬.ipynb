    "def solution(nums):\n",
    "    list=[]\n",
    "    \n",
    "    for i in nums:\n",
    "        if i not in list:\n",
    "            list.append(i)\n",
    "    if((len(nums)/2) <= len(list)):\n",
    "        answer=len(nums)/2\n",
    "    else:\n",
    "        answer=len(list)\n",
    "    \n",
    "    return answer"
   ]
